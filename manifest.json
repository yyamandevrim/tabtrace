{
  "manifest_version": 2,
  "name": "TabTrace",
  "version": "0.1",
  "description": "Track visited links with title and timestamp. Let NLP Categorize them.",
  "permissions": [
    "tabs",
    "history",
    "storage"
  ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup/popup.html"
  }
}
